{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": "true",
    "label": "Local BIG-IP declaration for standalone declarative onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "bigip-14a.tognaci.net",
        "myLicense": {
            "class": "License",
            "licenseType": "regKey",
            "regKey": "AAAAA-BBBBB-CCCCC-DDDDD-EEEEEEE"
        },
        "myProvisioning": {
            "class": "Provision",
            "ltm": "nominal"
        },
        "myDns": {
            "class": "DNS",
            "nameServers": [
                "8.8.8.8",
                "8.8.4.4"
            ],
            "search": [
                "f5.com",
                "tognaci.net"
            ]
        },
        "myNtp": {
            "class": "NTP",
            "servers": [
                "0.pool.ntp.org",
                "1.pool.ntp.org"
            ],
            "timezone": "CST"
        },
        "root": {
            "class": "User",
            "userType": "root",
            "oldPassword": "default",
            "newPassword": "default"
        },
        "admin": {
            "class": "User",
            "userType": "regular",
            "password": "admin",
            "shell": "bash"
        },
        "vince": {
            "class": "User",
            "userType": "regular",
            "password": "vince",
            "partitionAccess": {
                "Common": {
                    "role": "guest"
                }
            }
        },
        "external": {
            "class": "VLAN",
            "tag": 4094,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.1",
                    "tagged": false
                }
            ]
        },
        "internal": {
            "class": "VLAN",
            "tag": 4095,
            "mtu": 1500,
            "interfaces": [
                {
                    "name": "1.2",
                    "tagged": false
                }
            ]
        },
        "external-self": {
            "class": "SelfIp",
            "address": "10.1.10.140/24",
            "vlan": "external",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "internal-self": {
            "class": "SelfIp",
            "address": "10.1.20.140/24",
            "vlan": "internal",
            "allowService": "default",
            "trafficGroup": "traffic-group-local-only"
        },
        "default": {
            "class": "Route",
            "gw": "10.1.10.2",
            "network": "default",
            "mtu": 0
        }
    }
}